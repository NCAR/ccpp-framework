# dlc_liq is a scheme that has a ccpp_constituent_properties_t variable
#  outside of the register phase
[ccpp-table-properties]
  name = dlc_liq
  type = scheme
[ccpp-arg-table]
  name = dlc_liq_init
  type = scheme
[ dyn_const ]
  standard_name = dynamic_constituents_for_dlc_liq
  dimensions = (:)
  type = ccpp_constituent_properties_t
  intent = out
  allocatable = true
[ errmsg ]
  standard_name = ccpp_error_message
  long_name = Error message for error handling in CCPP
  units = none
  dimensions = ()
  type = character
  kind = len=512
  intent = out
[ errflg ]
  standard_name = ccpp_error_code
  long_name = Error flag for error handling in CCPP
  units = 1
  dimensions = ()
  type = integer
  intent = out
