<?xml version='1.0' encoding='utf-8'?>
<scheme module="gwdc">
  <subroutine name="gwdc_init" />
  <subroutine name="gwdc_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>iy</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>km</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>latitude_index_in_debug_printouts</name>
      <units>index</units>
      <id>lat</id>
      <rank>0</rank>
      <type>integer</type>
      <description>latitude index in debug printouts</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>x_wind</name>
      <units>m s-1</units>
      <id>u1</id>
      <rank>2</rank>
      <type>real</type>
      <description>zonal wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>y_wind</name>
      <units>m s-1</units>
      <id>v1</id>
      <rank>2</rank>
      <type>real</type>
      <description>meridional wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature</name>
      <units>K</units>
      <id>t1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>water_vapor_specific_humidity</name>
      <units>kg kg-1</units>
      <id>q1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer specific humidity of water vapor</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_physics</name>
      <units>s</units>
      <id>deltim</id>
      <rank>0</rank>
      <type>real</type>
      <description>physics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure</name>
      <units>Pa</units>
      <id>pmid1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_at_interface</name>
      <units>Pa</units>
      <id>pint1</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_difference_between_midlayers</name>
      <units>Pa</units>
      <id>dpmid1</id>
      <rank>2</rank>
      <type>real</type>
      <description>difference between mid-layer pressures</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>maximum_column_heating_rate</name>
      <units>K s-1</units>
      <id>qmax</id>
      <rank>1</rank>
      <type>real</type>
      <description>maximum heating rate in column</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_top</name>
      <units>index</units>
      <id>ktop</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud top</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_base</name>
      <units>index</units>
      <id>kbot</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud base</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_deep_convection</name>
      <units>index</units>
      <id>kcnv</id>
      <rank>1</rank>
      <type>integer</type>
      <description>flag indicating whether convection occurs in column (0 or 1)</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cloud_area_fraction</name>
      <units>frac</units>
      <id>cldf</id>
      <rank>1</rank>
      <type>real</type>
      <description>fraction of grid box area in which updrafts occur</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>gravitational_acceleration</name>
      <units>m s-2</units>
      <id>grav</id>
      <rank>0</rank>
      <type>real</type>
      <description>gravitational acceleration</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>specific_heat_of_dry_air_at_constant_pressure</name>
      <units>J kg-1 K-1</units>
      <id>cp</id>
      <rank>0</rank>
      <type>real</type>
      <description>specific heat of dry air at constant pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>gas_constant_dry_air</name>
      <units>J kg-1 K-1</units>
      <id>rd</id>
      <rank>0</rank>
      <type>real</type>
      <description>ideal gas constant for dry air</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>ratio_of_vapor_to_dry_air_gas_constants_minus_one</name>
      <units>none</units>
      <id>fv</id>
      <rank>0</rank>
      <type>real</type>
      <description>rv/rd - 1 (rv = ideal gas constant for water vapor)</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>pi</name>
      <units>radians</units>
      <id>pi</id>
      <rank>0</rank>
      <type>real</type>
      <description>ratio of a circle's circumference to its diameter</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>grid_box_horizontal_length_scale</name>
      <units>m</units>
      <id>dlength</id>
      <rank>1</rank>
      <type>real</type>
      <description>representative horizontal length scale of grid box</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_print</name>
      <units>flag</units>
      <id>lprnt</id>
      <rank>0</rank>
      <type>logical</type>
      <description>flag for debugging printouts</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_index_of_printed_column</name>
      <units>index</units>
      <id>ipr</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal index of column used in debugging printouts</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>forecast_time</name>
      <units>hr</units>
      <id>fhour</id>
      <rank>0</rank>
      <type>real</type>
      <description>forecast hour</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_x_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-2</units>
      <id>utgwc</id>
      <rank>2</rank>
      <type>real</type>
      <description>zonal wind tendency due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_y_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-2</units>
      <id>vtgwc</id>
      <rank>2</rank>
      <type>real</type>
      <description>meridional wind tendency due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>instantaneous_x_stress_due_to_gravity_wave_drag</name>
      <units>Pa</units>
      <id>tauctx</id>
      <rank>1</rank>
      <type>real</type>
      <description>zonal stress at cloud top due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>instantaneous_y_stress_due_to_gravity_wave_drag</name>
      <units>Pa</units>
      <id>taucty</id>
      <rank>1</rank>
      <type>real</type>
      <description>meridional stress at cloud top due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="gwdc_finalize" />
</scheme>
