<?xml version='1.0' encoding='utf-8'?>
<scheme module="gwdc_post">
  <subroutine name="gwdc_post_init" />
  <subroutine name="gwdc_post_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>levs</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_diagnostics</name>
      <units>flag</units>
      <id>lssav</id>
      <rank>0</rank>
      <type>logical</type>
      <description>flag for calculating diagnostic fields</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_diagnostics_3D</name>
      <units>flag</units>
      <id>ldiag3d</id>
      <rank>0</rank>
      <type>logical</type>
      <description>flag for calculating 3-D diagnostic fields</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_dynamics</name>
      <units>s</units>
      <id>dtf</id>
      <rank>0</rank>
      <type>real</type>
      <description>dynamics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_physics</name>
      <units>s</units>
      <id>dtp</id>
      <rank>0</rank>
      <type>real</type>
      <description>physics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>specific_heat_of_dry_air_at_constant_pressure</name>
      <units>J kg-1 K-1</units>
      <id>con_cp</id>
      <rank>0</rank>
      <type>real</type>
      <description>specific heat of dry air at constant pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>x_stress_at_cloud_top_due_to_convective_gravity_wave_drag</name>
      <units>Pa</units>
      <id>tauctx</id>
      <rank>1</rank>
      <type>real</type>
      <description>zonal stress at cloud top due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>y_stress_at_cloud_top_due_to_convective_gravity_wave_drag</name>
      <units>Pa</units>
      <id>taucty</id>
      <rank>1</rank>
      <type>real</type>
      <description>meridional stress at cloud top due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_x_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-2</units>
      <id>gwdcu</id>
      <rank>2</rank>
      <type>real</type>
      <description>zonal wind tendency due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_y_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-2</units>
      <id>gwdcv</id>
      <rank>2</rank>
      <type>real</type>
      <description>meridional wind tendency due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_integral_of_x_stress_at_cloud_top_due_to_convective_gravity_wave_drag  ??</name>
      <units>Pa s  ??</units>
      <id>dugwd  ??</id>
      <rank>2</rank>
      <type>real</type>
      <description>integral over time of zonal cloud top stress due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_integral_of_y_stress_at_cloud_top_due_to_convective_gravity_wave_drag  ??</name>
      <units>Pa s  ??</units>
      <id>dvgwd  ??</id>
      <rank>2</rank>
      <type>real</type>
      <description>integral over time of meridional cloud top stress due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cumulative_change_in_x_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-1  ??</units>
      <id>du3dt  ??</id>
      <rank>2</rank>
      <type>real</type>
      <description>cumulative change in zonal wind due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cumulative_change_in_y_wind_due_to_convective_gravity_wave_drag</name>
      <units>m s-1  ??</units>
      <id>dv3dt  ??</id>
      <rank>2</rank>
      <type>real</type>
      <description>cumulative change in meridional wind due to convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>updated_x_wind</name>
      <units>m s-1</units>
      <id>gu0</id>
      <rank>2</rank>
      <type>real</type>
      <description>updated zonal wind</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>updated_y_wind</name>
      <units>m s-1</units>
      <id>gv0</id>
      <rank>2</rank>
      <type>real</type>
      <description>updated meridional wind</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>updated_air_temperature</name>
      <units>m s-1</units>
      <id>gt0</id>
      <rank>2</rank>
      <type>real</type>
      <description>updated air temperature</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="gwdc_post_finalize" />
</scheme>
